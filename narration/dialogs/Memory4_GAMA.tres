[sub_resource type="Resource" id="Resource_th1h7"]
script = ExtResource("1_xll28")
speaker = "CALEB"
text = "Well, the last memory module is in place. It's ready."

[sub_resource type="Resource" id="Resource_lgig6"]
script = ExtResource("1_xll28")
speaker = "AVA"
text = "It's incredible. Do you think it will live up to our expectations?"

[sub_resource type="Resource" id="Resource_eo3vq"]
script = ExtResource("1_xll28")
speaker = "CALEB"
text = "I designed him to learn and adapt. He'll be more than a tool, he'll be part of our family."

[sub_resource type="Resource" id="Resource_nx6qs"]
script = ExtResource("1_xll28")
speaker = "SEARCH ENGINE ROBOT"
text = "System operational. I'm ready for my directives."

[sub_resource type="Resource" id="Resource_dm4li"]
script = ExtResource("1_xll28")
speaker = "ELI"
text = "Does it have a name?"

[sub_resource type="Resource" id="Resource_7pxem"]
script = ExtResource("1_xll28")
speaker = "AVA"
text = "Why not give him one? He's part of our team now."

[sub_resource type="Resource" id="Resource_u6xgm"]
script = ExtResource("1_xll28")
speaker = "ELI"
text = "I want to call him GAMA, like the explorer in the stories you tell me Mum, he'll help us discover new worlds!"

[sub_resource type="Resource" id="Resource_4qk4o"]
script = ExtResource("1_xll28")
speaker = "GAMA"
text = "GAMA\" (processing information): Identification accepted. I am ready to assist in discovery and exploration."

